{
  "name": "@majksa/svelte-operations",
  "version": "1.0.0-alpha12",
  "description": "GraphQL Code Generator plugin for processing query and mutation function and turn them into promises that request required fields.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-code-generator-community.git",
    "directory": "packages/plugins/typescript/svelte-operations"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.ts",
    "build": "yarn run build:types && yarn run build:esm && yarn run build:cjs",
    "build:types": "npm-dts generate -o ./dist/types/index.d.ts -f -L error",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^3.0.0",
    "@graphql-tools/apollo-engine-loader": "^6.2.5",
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "tslib": "~2.4.0"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typescript": {
    "definition": "dist/types/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^14.14.30",
    "npm-dts": "^1.3.9",
    "typescript": "^4.9.5"
  }
}
